          SQLTask:Connection="{02F4DEC9-E934-46CF-814B-DF703A9262EE}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="DECLARE @JobId int  SET @JobId =? IF(SELECT COUNT(*) FROM [SamplePatient].[dbo].[Job]) &gt; @JobId BEGIN -- Make sure that transaction works --&#xA;&#x9;DELETE FROM [SamplePatient].dbo.StoreChecksumSimplePatient WHERE JobId = (SELECT TOP 1 JobId FROM [SamplePatient].[dbo].[Job] ORDER BY JobId)&#xA;&#x9;DELETE FROM [SamplePatient].[dbo].[Job] WHERE JobId = (SELECT TOP 1 JobId FROM [SamplePatient].[dbo].[Job] ORDER BY JobId)&#xA;&#x9;INSERT INTO [dbo].[Job]&#xA;           ([JobDate]&#xA;           ,[JobTime]&#xA;           ,[JobStatus])&#xA;     VALUES&#xA;           (GETDATE()&#xA;           ,'12:46pm'&#xA;           ,'New')&#xA;&#x9;SELECT TOP 1 [JobId] as CurrentJobId&#xA;&#x9;FROM [SamplePatient].[dbo].[Job] WHERE [JobStatus] = 'New' ORDER BY JobId DESC&#xA;&#x9;END&#xA;&#xA;ELSE &#xA;BEGIN INSERT INTO [dbo].[Job]&#xA;           ([JobDate]&#xA;           ,[JobTime]&#xA;           ,[JobStatus])&#xA;     VALUES&#xA;           (GETDATE()&#xA;           ,'12:46pm'&#xA;           ,'New')&#xA;SELECT TOP 1 [JobId] as CurrentJobId&#xA;  FROM [SamplePatient].[dbo].[Job] WHERE [JobStatus] = 'New' ORDER BY JobId DESC&#xA;  END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
